{
"presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "@babel/stage-2"
  ],
  "plugins": ["@vue/babel-plugin-transform-vue-jsx", "@babel/plugin-transform-runtime"],
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-stage-2", "@vue/babel-preset-app"],
      "plugins": ["@vue/babel-plugin-transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
